<h1>Convert <%=@first_currency_symbol %></h1>

<p>Go <a href="/forex">back</a>.<p>

<%# p @array_of_symbols %>

<ul>
  <% @array_of_symbols.each do |a_symbol| %>
  <li>
    <a href="/forex/<%= @first_currency_symbol %>/<%= a_symbol %>">
      Convert 1  <%= @first_currency_symbol%> to <%= a_symbol %>...
    </a>
  </li>
  <% end %>
</ul>
